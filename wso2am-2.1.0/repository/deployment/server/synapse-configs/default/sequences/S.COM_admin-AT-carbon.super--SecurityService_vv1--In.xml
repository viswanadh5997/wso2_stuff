<?xml version="1.0" encoding="UTF-8"?>
<sequence xmlns="http://ws.apache.org/ns/synapse"
          name="S.COM/admin-AT-carbon.super--SecurityService:vv1--In">
   <property xmlns:soapenv="http://www.w3.org/2003/05/soap-envelope"
             xmlns:ns3="http://org.apache.synapse/xsd"
             name="apiResourcePath"
             expression="$ctx:REST_FULL_REQUEST_PATH"
             scope="default"
             type="STRING"
             description="apiResourcePath"/>
   <switch xmlns:soapenv="http://www.w3.org/2003/05/soap-envelope"
           xmlns:ns3="http://org.apache.synapse/xsd"
           source="get-property('apiResourcePath')">
      <case regex=".*ip/cus.*">
         <sequence key="gov:com/somos/services/v1/ip/customerservice/sequences/v1_customerserviceIn.xml"/>
      </case>
      <case regex=".*ip/num.*">
         <sequence key="gov:com/somos/services/v1/ip/numberservice/sequences/v1_numberserviceIn.xml"/>
      </case>
      <case regex=".*ip/sec.*">
         <sequence key="gov:com/somos/services/v1/ip/securityservice/sequences/v1_securityserviceIn.xml"/>
      </case>
      <case regex=".*ip/org.*">
         <sequence key="gov:com/somos/services/v1/ip/organizationservice/sequences/v1_organizationserviceIn.xml"/>
      </case>
      <default/>
   </switch>
</sequence>
