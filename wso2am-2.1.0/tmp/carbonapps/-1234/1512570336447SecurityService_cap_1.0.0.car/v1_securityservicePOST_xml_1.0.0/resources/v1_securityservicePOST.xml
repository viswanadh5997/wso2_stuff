<?xml version="1.0" encoding="UTF-8"?>
<sequence name="v1_securityservicePOST" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
  <log level="custom">
    <property name="In------------->" value="------------In--------------"/>
  </log>
  <class name="com.somos.wso2.operation.ValidateCredentials"/>
  <filter regex="true" source="get-property('handlerError')">
    <then>
      <log level="custom">
        <property name="beforeIF------------->" value="------------beforeIf--------------"/>
      </log>
      <sequence key="gov:com/somos/services/v1/ip/securityservice/sequences/v1_constructerrorResponse.xml"/>
    </then>
    <else/>
  </filter>
  <log level="custom">
    <property name="afterIn------------->" value="------------afterIn--------------"/>
  </log>
  <sequence key="gov:com/somos/services/v1/ip/securityservice/sequences/v1_assignHeaderDetails.xml"/>
</sequence>
