<?xml version="1.0" encoding="UTF-8"?>
<sequence name="v1_securityserviceWrapper" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
  <property description="apiResourcePath"
    expression="$ctx:REST_FULL_REQUEST_PATH" name="apiResourcePath"
    scope="default" type="STRING"/>
  <switch source="get-property('apiResourcePath')">
    <case regex=".*ip/cus.*">
      <sequence key="gov:com/somos/services/v1/ip/customerservice/sequences/v1_customerserviceIn.xml"/>
    </case>
    <case regex=".*ip/num.*">
      <sequence key="gov:com/somos/services/v1/ip/numberservice/sequences/v1_numberserviceIn.xml"/>
    </case>
    <case regex=".*ip/sec.*">
      <sequence key="gov:com/somos/services/v1/ip/securityservice/sequences/v1_securityserviceIn.xml"/>
    </case>
    <case regex=".*ip/org.*">
      <sequence key="gov:com/somos/services/v1/ip/organizationservice/sequences/v1_organizationserviceIn.xml"/>
    </case>
    <default/>
  </switch>
</sequence>
